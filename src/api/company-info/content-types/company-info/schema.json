{
  "kind": "singleType",
  "collectionName": "company_infos",
  "info": {
    "singularName": "company-info",
    "pluralName": "company-infos",
    "displayName": "companyInfo",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "logo": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "socialLinks": {
      "displayName": "socialLink",
      "type": "component",
      "repeatable": true,
      "component": "config.social-link"
    },
    "companyEmail": {
      "type": "email",
      "default": "email@example.com"
    }
  }
}
